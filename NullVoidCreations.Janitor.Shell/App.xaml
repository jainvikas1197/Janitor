<Application x:Class="NullVoidCreations.Janitor.Shell.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             ShutdownMode="OnExplicitShutdown">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/program_shell;component/Themes/SharedResources.xaml" />
                <ResourceDictionary Source="/program_shell;component/Themes/DefaultStyles.xaml" />
                <ResourceDictionary Source="/program_shell;component/Themes/NamedStyles.xaml" />
                <ResourceDictionary Source="/program_shell;component/Themes/LanguageStrings.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
